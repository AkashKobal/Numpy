## 8. Numerical Operations on numpy
### 8.1. Element wise operations

a = np.arange(10)
print(a+1)
print(a ** 2)
b = np.ones(10) + 1
print("b = ", b)
print("a - b = ",a-b)
print(a*b)


#Matrix Multiplication

c = np.diag([1,2,3,4])
print(c)
print("*"*100)
print(c*c)
print("*"*100)
print(c.dot(c))


# element comparisions

a = np.array([1,2,5,4])
b = np.array([6,2,9,4])
print(a==b)

print(a>b)
print(a<b)
print(a<=b)

#array wise comparision

print(np.array_equal(a,b))
c = np.array([1,2,5,4])
print(np.array_equal(a,c))
